{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "es2015"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "lib",
      "experimentalDecorators": true,
      "suppressImplicitAnyIndexErrors": true,
      "allowSyntheticDefaultImports": true,
      "declaration": false,
      "strict": true,
      "removeComments": true,
      "noImplicitAny": false,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "resa": ["src/index.ts"]
      },
      "jsx": "react"
    },
    "files": [
      "node_modules/jest-dom/extend-expect.d.ts",
    ],
    "include": [
      "src/**/*.tsx",
      "src/**/*.ts",
      "src/*.tsx",
      "src/*.ts",
    ],
    "compileOnSave": false
}
